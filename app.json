{
  "expo": {
    "name": "museumku",
    "slug": "museumku",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "museumku",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,  // UBAH JADI FALSE (Demi kestabilan)
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.navico.museumku"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "package": "com.navico.museumku",
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": false  // UBAH JADI FALSE (Ini biang kerok error aneh)
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "61c1cda6-d046-4a81-82da-3591dede317e"
      }
    }
  }
}